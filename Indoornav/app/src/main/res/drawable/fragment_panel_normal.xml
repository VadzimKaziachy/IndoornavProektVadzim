            android:innerRadius="0dp"
            android:thickness="70dp"
            android:useLevel="false" >
            <!--<stroke-->
                <!--android:width="4dp"-->
                <!--android:color="@color/borderLow">-->
            <!--</stroke>-->
            <gradient
                android:centerY="0.0"
                android:endColor="@color/gradienEnd"
                android:gradientRadius="500dp"
                android:startColor="@color/gradientStart">
            </gradient>
        </shape>
    </item>
