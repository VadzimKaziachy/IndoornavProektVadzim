<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.indoornav">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="by.grsu.ftf.indoornav.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="by.grsu.ftf.indoornav.beaconInfo.FragmentActivity" />
        <activity android:name="by.grsu.ftf.indoornav.navigation.map.MapActivity" />
        <activity android:name="by.grsu.ftf.indoornav.administrator.activitySearch.SearchActivity" />
        <activity android:name="by.grsu.ftf.indoornav.administrator.activityEntry.EntryActivity"></activity>
    </application>

</manifest>