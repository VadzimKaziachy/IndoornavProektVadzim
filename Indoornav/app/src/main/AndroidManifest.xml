<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.indoornav">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="by.grsu.ftf.indoornav.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name="by.grsu.ftf.indoornav.navigation.map.MapProcessor"/>
        <service android:name="by.grsu.ftf.indoornav.widget.PathCalculator"/>
        <service android:name="by.grsu.ftf.beacomlib.Bluetooth"/>
        <receiver android:name="by.grsu.ftf.indoornav.Broadcast">
            <intent-filter>
                <action android:name="KEY_INTENT_FILTER" />
            </intent-filter>
        </receiver>
    </application>

</manifest>